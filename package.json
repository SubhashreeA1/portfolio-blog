{
  "name": "portfolio-blog",
  "version": "1.0.0",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "start-client": "cd client && npm run dev",
    "start-server": "cd server && npm run dev",
    "dev": "concurrently \"npm run start-client\" \"npm run start-server\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
